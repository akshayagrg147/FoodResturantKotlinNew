<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.orders.resturantorder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.orders.resturantorder.MyRoomRetroApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Demo">
        <activity android:name="com.orders.resturantorder.Activity.SplashScreenActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">

        </activity>
        <activity android:name="com.orders.resturantorder.Activity.MobileNumberIntegration"  android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">


        </activity>
        <activity
            android:name="com.orders.resturantorder.Activity.AddNewAddressActivity"
            android:label="Add New Address" />
        <activity
            android:name="com.orders.resturantorder.Activity.ProceedToAddress"
            android:label="Confirm Address" />
        <activity android:name="com.orders.resturantorder.Activity.CartItemss" />
        <activity
            android:name="com.orders.resturantorder.Activity.AfterCategorySelectionActivity"
            android:label="Product List" />
        <activity android:name="com.orders.resturantorder.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
    </application>

</manifest>